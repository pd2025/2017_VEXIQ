task main()
{
	moveMotor(clawMotor, 2.5, seconds, 50);
	moveMotor(armMotor, 1, rotations, 50);
	forward(10, rotations, 50);
	setTouchLEDColor(touchLED, colorRedViolet);
	moveMotor(armMotor, 1, rotations, -50);
	moveMotor(armMotor, 1, rotations, 50);
	backward(1, rotations, 50);
	turnRight(180, degrees, 50);
	forward(3, rotations, 50);
	turnLeft(270, degrees, 50);
	forward(1.5, rotations, 50);
	moveMotor(armMotor, -1, rotations, -50);
}
